function searchForm(){
	var search = $("#search").val();
	var filter = $('input[name=filter]:checked', '#form1').val();
	if (search.trim().length > 0) {
		var url = "/search?q=" + search;
		if (filter != "all") {
			url += "&filter=" + filter;
		}
		window.location = url;
	}
}

