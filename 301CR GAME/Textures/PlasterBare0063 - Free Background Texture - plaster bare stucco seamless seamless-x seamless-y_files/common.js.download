function createGoogleAdsenseScript(adsType) {
	var completeScript = '';

	if(_globalAdsScriptAdded === false) {
		completeScript += "<script async='async' src='//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js'></script>\n";
